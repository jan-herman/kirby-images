{
    "name": "jan-herman/kirby-images",
    "description": "Opinionated set of Kirby snippets and utilities for serving highly optimized versions of images.",
    "version": "1.4.0",
    "type": "kirby-plugin",
    "license": "MIT",
    "homepage": "https://github.com/jan-herman/kirby-images",
    "authors": [
        {
            "name": "Jan Herman",
            "email": "mail@janherman.com",
            "homepage": "https://janherman.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "getkirby/cms": "^4.0 || ^5.0",
        "getkirby/composer-installer": "^1.2"
    },
    "extra": {
        "installer-name": "images"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "JanHerman\\Images\\": "lib/"
        }
    }
}